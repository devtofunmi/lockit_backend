generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Message {
  id        String   @id @default(cuid())
  content   String
  viewed    Boolean  @default(false)
  createdAt DateTime @default(now())
  expiresAt DateTime
}


